#!/usr/bin/make -f
export DH_VERBOSE = 1

%:
	dh $@

override_dh_clean:
	rm -rf ./debian/whitesur-gtk-theme
	dh_clean

override_dh_auto_build:
	mkdir ./debian/whitesur-gtk-theme
	./install.sh -d "$(CURDIR)/debian/whitesur-gtk-theme/usr/share/themes"
	dh_auto_build

override_dh_prep:
	echo "override dh_prep"
